# Environment Configuration Example
# Copy this file to .env and modify the values according to your setup

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=disaster_management
DB_USER=disaster_user
DB_PASSWORD=disaster_password

# Backend API Configuration
API_BASE_URL=http://localhost:8080
API_URL=http://localhost:8080
API_VERSION=v1

# WebSocket Configuration
WEBSOCKET_URL=http://localhost:8086/ws

# Redis Configuration (if using)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRATION=24h

# Application Configuration
NODE_ENV=development
PORT=4200

# Docker Configuration
COMPOSE_PROJECT_NAME=disaster-management-system
